<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="Word">
    <select id="Word" resultType="CrwalingCacheDTO">
        SELECT "Name(News)", "URL(Now)", "Date(Now)", "Head(Now)", "Largest1(Now)","Largest1(Now_number)",
            "Largest2(Now)","Largest2(Now_number)","Largest3(Now)","Largest3(Now_number)"
             ,"Largest4(Now)","Largest4(Now_number)","Largest5(Now)","Largest5(Now_number)","Entire(Now)"
             ,"Entire(Now_number)"
        FROM "Crwaling(Now)"
    </select>

